{
  "version": "0.0.1",
  "name": "gatsby-source-graphcms",
  "description": "Gatsby source plugin for building websites using the GraphCMS as a data source.",
  "keywords": [
    "gatsby",
    "graphcms"
  ],
  "homepage": "https://github.com/GraphCMS/gatsby-source-graphcms",
  "repository": {
    "type": "git",
    "url": "https://github.com/GraphCMS/gatsby-source-graphcms.git"
  },
  "bugs": {
    "url": "https://github.com/GraphCMS/gatsby-source-graphcms/issues"
  },
  "license": "MIT",
  "author": "Angel Piscola <angelpiscola@gmail.com>",
  "deprecated": false,
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "lint": "xo",
    "format": "xo --fix",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "cross-env": "5.1.1",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "xo": "0.18.2"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "graphql-request": "^1.3.4",
    "ramda": "^0.25.0"
  },
  "xo": {
    "space": true,
    "quotes": "backtick",
    "parser": "babel-eslint",
    "overrides": [
      {
        "files": "src/__tests__/*.js",
        "globals": [
          "it",
          "expect"
        ]
      }
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "labels": [
      "renovate"
    ],
    "assignees": [
      "rdela"
    ]
  }
}
